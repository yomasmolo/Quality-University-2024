    "cypress:run:smoke": "cypress run --spec 'cypress/integration/smoke_tests/*'",
    "cypress:run:form": "cypress run --spec 'cypress/e2e/form.cy.js'",
    "cypress:run:report": "cypress run --reporter mochawesome",
    "cypress:test:report": "cypress run --browser chrome --headless --reporter mochawesome --reporter-options reportDir=cypress/reports"
  },
